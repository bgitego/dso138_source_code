 405:Command.c     **** 			}
 406:Command.c     **** /*		
 407:Command.c     **** 		// Make key debounce time shorter for these TB's 
 408:Command.c     **** 		if((tmp0 < TB_20ms) && (tmp0 > TB_1s)) {
 409:Command.c     **** 			Delay(50000);	// To avoid same keypress repeated
 410:Command.c     **** 			Keypad.KDebounceVal = KD_val1;
