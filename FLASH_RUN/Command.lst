 421:Command.c     ****  		tmp0--;
 422:Command.c     ****  		tmp0 = SetTrigMode(tmp0);
 423:Command.c     **** 		EE_WriteVariable(VirtAddVarTab[Addr_TrigMode], tmp0);
 424:Command.c     **** 		// Restart capture. 
 425:Command.c     **** 		StartCapture();
