 411:Eeprom.c      **** 
 412:Eeprom.c      ****   /* Get Page1 actual status */
 413:Eeprom.c      ****   PageStatus1 = (*(__IO uint16_t*)PAGE1_BASE_ADDRESS);
 414:Eeprom.c      **** 
 415:Eeprom.c      ****   /* Write or read operation */
 416:Eeprom.c      ****   switch (Operation)
